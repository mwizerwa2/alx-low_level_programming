#include <stdlib.h>
#include <time.h>
/* more headers goes there */

/* betty style doc for function main goes there */
int main(void)
{
	int n;
	
	scanf("%d", &n);
if(n>=1)
{
printf("%d=n is positive/n", n);
}
else{
printf("%d=n is negative/n", n);
}
	

	srand(time(0));
	n = rand() - RAND_MAX / 2;
	/* your code goes there */
	return (0);
}
